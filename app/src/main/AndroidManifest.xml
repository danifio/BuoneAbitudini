<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.java.buoneabitudini"
    xmlns:buoneabitudini="http://schemas.android.com/apk/res-auto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_tastiera"
        buoneabitudini:showAsAction="ifRoom"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity2"
            android:parentActivityName=".MainActivity"
            >

        </activity>
        <activity android:name=".MainActivity2_2"
            android:parentActivityName=".MainActivity2"
            >

        </activity>
        <activity android:name=".MainActivity3"
            android:parentActivityName=".MainActivity2_2">

        </activity>
        <activity android:name=".MainActivity4"
            android:parentActivityName=".MainActivity3">

        </activity>
        <activity android:name=".MainActivity4_2"
            android:parentActivityName=".MainActivity4">

        </activity>
        <activity android:name=".MainActivity5"
            android:parentActivityName=".MainActivity4_2">

        </activity>
    </application>

</manifest>
